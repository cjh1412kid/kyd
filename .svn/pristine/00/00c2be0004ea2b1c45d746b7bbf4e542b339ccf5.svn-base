<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.nuite.modules.system.dao.MeetingAreaDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.nuite.modules.system.entity.MeetingAreaEntity" id="meetingAreaMap">
        <result property="seq" column="Seq"/>
        <result property="areaName" column="AreaName"/>
        <result property="companySeq" column="Company_Seq"/>
        <result property="inputTime" column="InputTime"/>
        <result property="del" column="Del"/>
    </resultMap>
	<select id="getMeetingAreaByName" resultMap="meetingAreaMap">
	SELECT * FROM [YHSROrderPlatform].[dbo].[YHSR_OP_MeetingArea] where AreaName=#{name} AND Del=0
	</select>

</mapper>